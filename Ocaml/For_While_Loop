for i = 0 to 10 do
    print_string "value of i is: ";
    print_int i;
    print_string "\n";
done;;



for i = 0 downto 0 do
    print_string "value of i is: ";
    print_int i;
    print_string "\n";
done;;



let i = 4;;

while i < 5 do
    print_int i;
done


=    checking structural equality
==   checking physical equality
