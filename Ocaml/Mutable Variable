let x = 5;; (*costant*)

let x = ref 5;;(*ref allows x to be changed later*)

x;; (*gets ref*)

!x;; (*get actual value*)

x := 3;; (*update x*)

while !x < 10 do
print_string "Hello World\n";
x := !x + 1;
done;;



let f1' = ref 3.4;;

let f2' = ref 3.4;;

f1' = f2';; (*true*)

f1' == f2';; (*false*)
